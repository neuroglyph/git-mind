{
  "name": "gitmind",
  "version": "0.1.0",
  "description": "Git as a distributed knowledge graph",
  "private": true,
  "workspaces": [
    "server",
    "webapp"
  ],
  "scripts": {
    "start": "npm run start:server",
    "start:server": "cd server && npm start",
    "install:all": "npm install && cd server && npm install",
    "dev": "cd server && npm run dev",
    "test": "echo \"Test suite not yet implemented\"",
    "docker:test": "docker-compose -f docker-compose.test.yml up",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/gitmind.git"
  },
  "keywords": [
    "git",
    "knowledge-graph",
    "distributed",
    "semantic"
  ],
  "author": "Gitmind Team",
  "license": "Apache-2.0",
  "devDependencies": {}
}